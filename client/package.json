{
    "scripts": {
        "analyse": "webpack --env analyse",
        "build": "webpack",
        "clean": "rm -rf ../node_modules/.cache/@x/front-end-client ./public/assets/*",
        "extract": "formatjs extract \"src/**/*.{ts,vue}\" --format simple --out-file src/app/site/lang/en-gb.json",
        "format-check": "prettier --check \"**/*.{js,json,ts,vue}\"",
        "format": "prettier --write \"**/*.{js,json,ts,vue}\"",
        "lint:scripts": "eslint \"src/**/*.{ts,vue}\" \"*.js\"",
        "lint:styles": "stylelint \"src/**/*.{scss,vue}\"",
        "lint": "concurrently \"npm:lint:*\"",
        "postextract": "prettier --write src/app/site/lang/en-gb.json",
        "spell-check": "cspell",
        "start": "webpack serve --hot --mode=development",
        "type-check": "tsc -p . --noEmit",
        "watch": "webpack --watch --mode=development"
    },
    "dependencies": {
        "vue": "^3.0.11",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-intl": "^4.1.26",
        "vue-meta": "^3.0.0-alpha.3",
        "vue-property-decorator": "^10.0.0-rc.2",
        "vue-router": "^4.0.6",
        "vuetify": "^3.0.0-alpha.2"
    },
    "devDependencies": {
        "@formatjs/cli": "^4.2.6",
        "@formatjs/ts-transformer": "^3.3.6",
        "@mdi/font": "^5.9.55",
        "@mdi/js": "^5.9.55",
        "@types/browser-sync-webpack-plugin": "^2.2.0",
        "@types/connect-history-api-fallback": "^1.3.4",
        "@types/css-minimizer-webpack-plugin": "^1.1.2",
        "@types/imagemin-gifsicle": "^7.0.0",
        "@types/imagemin-jpegtran": "^5.0.0",
        "@types/imagemin-optipng": "^5.2.0",
        "@types/imagemin-svgo": "^9.0.0",
        "@types/mini-css-extract-plugin": "^1.2.2",
        "@types/terser-webpack-plugin": "^5.0.2",
        "@types/webpack-bundle-analyzer": "^3.9.1",
        "@vue/compiler-sfc": "^3.0.11",
        "autoprefixer": "^10.2.5",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.3.0",
        "concurrently": "^6.0.1",
        "connect-history-api-fallback": "^1.6.0",
        "css-loader": "^5.1.3",
        "css-minimizer-webpack-plugin": "^1.3.0",
        "fibers": "^5.0.0",
        "file-loader": "^6.2.0",
